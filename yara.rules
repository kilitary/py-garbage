rule gzip_embed
{
    meta:
        description = "gzip embedded"

    strings:
        $a = {1f 8b 08 00}

    condition:
        $a
}

rule gzip_mult
{
    meta:
        description = "gzip multiple"

    strings:
        $a = {1f 8b 08 00}

    condition:
        #a > 1
}

