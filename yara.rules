rule gzip_embed
{
    meta:
        description = "gzip embedded"

    strings:
        $a = {1f 8b 08 00}

    condition:
        all of them
}

rule gzip_mult
{
    meta:
        description = "gzip multiple sections"

    strings:
        $a = {1f 8b 08 00}

    condition:
        #a > 1
}

rule chertocrypt
{
    meta:
        description = "4ertOcrypt"

    strings:
        $a = "4ertKcrypt"
        $b = "SGN"

    condition:
        all of them
}

rule upx
{
	meta:
		description = "upx packer"

	strings:
		$a = {?? 55 50 58 21}

	condition:
		all of them
}

rule uac
{
	meta:
		description = "uac elevation (admin)"

	strings:
		$a = "level=\"highestAvailable\""

	condition:
		all of them
}
